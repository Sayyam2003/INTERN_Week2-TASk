# Store 5 student names in a list
students = ["Sayyam", "Ahmed", "Ali", "Zawar", "Shahzeb"]

# Print each student name
for name in students:
    print(name)

#For reversing list
# Original list
students = ["Sayyam", "Ali", "Zawar", "Ahmed", "Shahzeb"]

# Reverse the list manually using slicing
reversed_students = students[::-1]

# Print reversed list
print("Reversed list:")
for name in reversed_students:
    print(name)

